@model IEnumerable<ConsumirApi.Models.Usuarios>

@{
    ViewBag.Title = "index";
}

<h2>index</h2>

<p>
    @Html.ActionLink("Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>id</th>
        <th>Estado</th>
        <th>Fecha</th>
        <th>Nombre</th>
        <th>Direccion</th>
        <th>Departamento</th>
        <th>Telefono</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str_status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dat_fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str_nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str_direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str_departamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.int_telefono)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { /*id = item.Id*/ }) |
            @Html.ActionLink("Delete", "Delete", new { /*id = item.Id*/ })
        </td>
    </tr>
}

</table>
